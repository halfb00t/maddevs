{
  "name": "maddevs",
  "version": "2.0.0",
  "description": "Mad Devs",
  "author": "Mad Devs",
  "private": true,
  "engines": {
    "node": ">=15 <17"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "npm run lint-vue-scss & npm run lint-scss & npm run lint-es ",
    "lint-es": "eslint -c .eslintrc --ext .js,.vue --ignore-path .gitignore .",
    "lint-vue-scss": "sass-lint-vue components",
    "lint-scss": "sass-lint client/assets/styles/*.scss --verbose",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "heroku-cleanup": "cd ./client; ls | grep -v static | xargs rm -rfv",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "node ./jest",
    "test:unit:node": "node ./jest -- --test=node",
    "test:unit:browser": "node ./jest -- --test=browser",
    "test:staging": "node ./jest && node ./jest/utils/sendCoverageToSlack.js",
    "test:e2e": "cypress run",
    "generate:coverage-badges": "node ./jest/utils/generateCoverageBadges.js -output './client/static'",
    "seo-analyze": "npm run build && npm run generate && node ./SEOAnalyzer/index.js",
    "slicemachine": "start-slicemachine --port 9999",
    "storybook": "nuxt storybook",
    "build-storybook": "nuxt storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.32.0",
    "@aws-sdk/s3-request-presigner": "^3.32.0",
    "@maddevs/mad-radiator": "1.17.5",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/prismic": "^1.3.1",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.0",
    "@sentry/node": "^6.3.1",
    "@sentry/tracing": "^6.2.3",
    "@sentry/vue": "^6.3.1",
    "aos": "^2.3.4",
    "axios": "^0.21.3",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "cli-progress": "^3.9.0",
    "cors": "^2.8.5",
    "countup.js": "^2.0.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "geoip-lite": "^1.4.2",
    "img-comparison-slider": "^7.2.0",
    "lottie-web": "^5.7.13",
    "mobile-device-detect": "^0.4.3",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nuxt": "2.15.8",
    "nuxt-i18n": "^6.28.0",
    "nuxt-lazy-load": "^1.2.6",
    "nuxt-sm": "0.0.8",
    "prismjs": "^1.24.1",
    "request": "^2.88.2",
    "sendpulse-api": "^1.1.4",
    "simplebar-vue": "^1.6.2",
    "slice-machine-ui": "0.1.0",
    "snyk": "^1.717.0",
    "swiper": "^5.4.5",
    "uuid": "^8.3.2",
    "vue-awesome-swiper": "^4.1.1",
    "vue-disqus": "^4.0.1",
    "vue-embed-gist": "^1.1.0",
    "vue-lottie": "^0.2.1",
    "vue-parallax-js": "^0.2.3",
    "vue-prism-editor": "^1.2.2",
    "vue-prlx": "^1.3.4",
    "vue-select": "^3.11.2",
    "vue-slicezone": "^0.1.0",
    "vue-social-sharing": "^3.0.7",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime-corejs3": "^7.14.9",
    "@cypress/vue": "^2.2.3",
    "@nuxtjs/storybook": "3.3.1",
    "@storybook/vue": "6.1.21",
    "@testing-library/vue": "^5.6.1",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/test-utils": "^1.1.0",
    "async": "^3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.5.1",
    "chalk": "^4.1.1",
    "core-js": "^3.16.0",
    "cypress": "^7.7.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^4.3.0",
    "istanbul-api": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-serializer-vue": "^2.0.2",
    "mkdirp": "^1.0.4",
    "node-sass": "^6.0.1",
    "regenerator-runtime": "^0.13.7",
    "sass-lint": "^1.13.1",
    "sass-lint-vue": "^0.4.0",
    "sass-loader": "^10.1.1",
    "seo-analyzer": "^1.0.6",
    "vue-jest": "^3.0.7",
    "vue-loader": "15.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "snyk": true
}
